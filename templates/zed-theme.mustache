    "tab_bar.background": "#{{surface-hex}}",
    "tab.inactive_background": "#{{background-hex}}",
    "tab.active_background": "#{{overlay-hex}}",
    "panel.background": "#{{surface-hex}}",
    "scrollbar.thumb.background": "#{{overlay-hex}}33",
    "scrollbar.thumb.hover_background": "#{{overlay-hex}}77",
    "scrollbar.thumb.border": "#{{overlay-hex}}33",
    "scrollbar.track.background": "#{{surface-hex}}",
    "scrollbar.track.border": "#00000000",
    "editor.foreground": "#{{base07-hex}}",
    "editor.background": "#{{background-hex}}",
    "editor.gutter.background": "#{{background-hex}}",
    "editor.active_line.background": "#{{surface-hex}}",
    "editor.highlighted_line.background": "#{{surface-hex}}",
    "editor.line_number": "#{{base05-hex}}",
    "editor.active_line_number": "#{{base07-hex}}",
    "editor.wrap_guide": "#00000000",
    "editor.active_wrap_guide": "#00000000",
