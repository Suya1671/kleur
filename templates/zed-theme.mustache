    "terminal.ansi.red": "#{{red-hex}}",
    "terminal.ansi.bright_red": "#{{red-bright-hex}}",
    "terminal.ansi.green": "#{{green-hex}}",
    "terminal.ansi.bright_green": "#{{green-bright-hex}}",
    "terminal.ansi.yellow": "#{{orange-hex}}",
    "terminal.ansi.bright_yellow": "#{{orange-bright-hex}}",
    "terminal.ansi.blue": "#{{blue-hex}}",
    "terminal.ansi.bright_blue": "#{{blue-bright-hex}}",
    "terminal.ansi.magenta": "#{{purple-hex}}",
    "terminal.ansi.bright_magenta": "#{{purple-bright-hex}}",
    "terminal.ansi.cyan": "#{{teal-hex}}",
    "terminal.ansi.bright_cyan": "#{{teal-bright-hex}}",
    "terminal.ansi.white": "#{{base07-hex}}",
    "terminal.ansi.bright_white": "#{{primary-hex}}",
    "link_text.hover": "#{{teal-hex}}",
    "conflict": "#{{red-hex}}",
    "created": "#{{green-hex}}",
    "deleted": "#{{red-hex}}",
    "error": "#{{red-hex}}",
    "hidden": "#{{base05-hex}}",
    "hint": "#{{base05-hex}}",
    "hint.background": "#{{overlay-hex}}",
    "ignored": "#{{base05-hex}}",
    "info": "#{{base-hex}}",
    "info.background": "#{{primary-200-hex}}",
    "modified": "#{{orange-hex}}",
    "warning": "#{{orange-hex}}",
    "players": [],
    "syntax": {
      "namespace": {
        "color": "#{{green-hex}}"
      },
      "class": {
        "color": "#{{blue-hex}}"
      },
      "class.abstract": {
        "color": "#{{orange-hex}}"
      },
      "enum": {
        "color": "#{{green-hex}}"
      },
      "interface": {
        "color": "#{{orange-hex}}"
      },
      "struct": {
        "color": "#{{blue-hex}}"
      },
      "typeParameter": {
        "color": "#{{green-hex}}"
      },
      "type": {
        "color": "#{{blue-hex}}"
      },
      "parameter": {
        "color": "#{{purple-hex}}"
      },
      "variable": {
        "color": "#{{purple-hex}}"
      },
      "variable.builtin": {
        "color": "#{{green-hex}}"
      },
      "property": {
        "color": "#{{purple-hex}}"
      },
      "enumMember": {
        "color": "#{{green-hex}}"
      },
      "decorator": {
        "color": "#{{blue-hex}}"
      },
      "event": {
        "color": "#{{blue-hex}}"
      },
      "punct": {
        "color": "#{{base08-hex}}"
      },
      "punctuation": {
        "color": "#{{base08-hex}}"
      },
      "punctuation.bracket": {
        "color": "#{{primary-hex}}"
      },
      "escapeSequence": {
        "color": "#{{red-hex}}"
      },
      "function": {
        "color": "#{{blue-hex}}",
        "font_style": "italic"
      },
      "method": {
        "color": "#{{blue-hex}}",
        "font_style": "italic"
      },
      "macro": {
        "color": "#{{blue-hex}}"
      },
      "label": {
        "color": "#{{base06-hex}}"
      },
      "comment": {
        "color": "#{{base03-hex}}",
        "font_style": "italic"
      },
      "string": { "color": "#{{green-hex}}" },
      "keyword": {
        "color": "#{{purple-hex}}",
        "font_style": "italic"
      },
      "keyword.control": {
        "color": "#{{purple-hex}}",
        "font_style": "italic"
      },
      "number": {
        "color": "#{{orange-hex}}"
      },
      "string.regex": {
        "color": "#{{orange-hex}}"
      },
      "boolean": {
        "color": "#{{orange-hex}}"
      },
      "bool": {
        "color": "#{{orange-hex}}"
      },
      "const": {
        "color": "#{{orange-hex}}"
      },
      "constant": {
        "color": "#{{orange-hex}}"
      },
      "operator": {
        "color": "#{{purple-hex}}"
      },
      "tag": {
        "color": "#{{purple-hex}}"
      },
      "attribute": {
        "color": "#{{blue-hex}}"
      },
      "function.declaration": {
        "color": "#{{purple-hex}}",
        "font_weight": 700
      },
      "enum.declaration": {
        "color": "#{{green-hex}}",
        "font_weight": 700
      },
      "class.declaration": {
        "color": "#{{blue-hex}}",
        "font_weight": 700
      },
      "method.declaration": {
        "color": "#{{blue-hex}}",
        "font_weight": 700
      },
      "trait.declaration": {
        "color": "#{{orange-hex}}",
        "font_weight": 700
      },
      "struct.declaration": {
        "color": "#{{blue-hex}}",
        "font_weight": 700
      },
      "function.async": {
        "color": "#{{orange-hex}}"
      },
      "unresolvedReference": {
        "color": "#{{red-hex}}"
      },
      "namespace.defaultLibrary": {
        "color": "#{{green-hex}}"
      },
      "variable.defaultLibrary": {
        "color": "#{{green-hex}}"
      },
      "namespace.builtin": {
        "color": "#{{green-hex}}"
      },
      "emphasis": {
        "color": "#{{primary-hex}}",
        "font_style": "italic"
      },
      "emphasis.strong": {
        "color": "#{{primary-hex}}",
        "font_style": "italic",
        "font_weight": 700
      }
    }
  }
}
